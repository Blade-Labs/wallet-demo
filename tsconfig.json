{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "incremental": false,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "noUnusedParameters": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "vite/client",
      "node"
    ],
    "lib": [
      "esnext",
      "dom"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
    },
  },
  "exclude": [
    "dist",
  ],
  "include": [
    "@types",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "auto-imports.d.ts"
  ]
}
